{
    "name": "loureirobruno/buscador-cursos",
    "description": "Biblioteca que busca os cursos no site da alura\"",
    "type": "library",
    "autoload": {
        "files": [
            "./funcoes.php"
        ],
        "classmap": [
            "./src/ClasseLegada.php",
            "./ClasseLegada_.php"
        ],
        "psr-4": {
            "LoureiroBruno\\BuscadorDeCursos\\": "src/"
        }
    },
    "authors": [
        {
            "name": "LoureiroBruno",
            "email": "brunoloureiro9@yahoo.com.br"
        }
    ],
    "require": {
        "guzzlehttp/guzzle": "^7.4",
        "symfony/dom-crawler": "^6.1",
        "symfony/css-selector": "^6.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8"
    }
}
